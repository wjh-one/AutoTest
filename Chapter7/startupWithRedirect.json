[
  {
    "description": "重定向到百度",
    "request": {
      "uri": "/redirect"
    },
    "redirectTo": "http://www.baidu.com"
  },
  {
    "description": "重定向到自己的一个网页",
    "request": {
      "uri": "/redirect/to"
    },
    "redirectTo": "/redirect/path"
  },
  {
    "description": "被重定向的地址",
    "request": {
      "uri": "/redirect/path"
    },
    "response": {
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "text": "重定向成功！"
    }
  }
]